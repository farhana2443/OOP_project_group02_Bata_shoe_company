<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop_project_group02_bata_shoe_company.SulthanaTasnim.Customer.CustomerDashboard">
   <children>
      <VBox layoutX="16.0" layoutY="73.0" prefHeight="283.0" prefWidth="220.0">
         <children>
            <Button fx:id="browseShoesbyCategoryBTN" mnemonicParsing="false" onAction="#viewBrowseShoesbyCategorypanel" prefHeight="26.0" prefWidth="246.0" text="Browse Shoes by Category" />
            <Button fx:id="searchSpecificShoeModelBTN" mnemonicParsing="false" onAction="#viewsearchSpecificShoeModelPanel" prefHeight="26.0" prefWidth="242.0" text="Search Specific Shoe Model" />
            <Button fx:id="viewProductDetailsandReviewsBTN" mnemonicParsing="false" onAction="#viewProductDetailsandReviewsPanel" prefHeight="26.0" prefWidth="241.0" text="View Product Details &amp; Reviews" />
            <Button fx:id="addProducttoCartBTN" mnemonicParsing="false" onAction="#viewAddProducttoCartButtonPanel" prefHeight="26.0" prefWidth="241.0" text="Add Product to Cart" />
            <Button fx:id="chooseDeliveryorPickupOptionBTN" mnemonicParsing="false" onAction="#ViewChooseDeliveryorPickupOptionPannel" prefHeight="26.0" prefWidth="243.0" text="Choose Delivery or Pickup Option" />
            <Button fx:id="provideShippingandContactDetailsBTN" mnemonicParsing="false" onAction="#viewProvideShippingandContactDetailspanel" prefHeight="26.0" prefWidth="249.0" text="Provide Shipping and Contact Details" />
            <Button fx:id="applyCouponandMakePaymentBTN" mnemonicParsing="false" onAction="#viewApplyCouponandMakePaymentPanel" prefHeight="26.0" prefWidth="242.0" text="Apply Coupon and Make Payment" />
            <Button fx:id="trackOrderandRequestReturnBTN" mnemonicParsing="false" onAction="#viewTrackOrderandRequestReturnPanel" prefHeight="26.0" prefWidth="245.0" text="Track Order and Request Return" />
         </children>
      </VBox>
      <Label layoutX="16.0" layoutY="21.0" prefHeight="42.0" prefWidth="242.0" text="Customer panels">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
   </children>
</AnchorPane>
